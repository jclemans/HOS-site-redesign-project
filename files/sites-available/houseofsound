upstream houseofsound {
        server 127.0.0.1:10000;
}

server {
#       server_name schedule.houseofsound.org;
        server_name houseofsound.org *.houseofsound.org;
        listen 80; 
        access_log /var/log/nginx/houseofsound.access.log;

        location / { 
                client_max_body_size 500M;
                proxy_pass http://houseofsound;
                break;
        }
}
