upstream fbmusic {
        server 127.0.0.1:10010;
}

server {
        server_name fbmusic.houseofsound.org
        listen 80; 
        access_log /var/log/nginx/fbmusic.access.log;

        location / { 
                client_max_body_size 50M;
                proxy_pass http://fbmusic;
                break;
        }
    
        limit_rate_after 150k;
        limit_rate 50k;
        # set $limit_rate  3m; 
}
